<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean id="calculator" class="com.koreait.quiz1.Calculator"/>

<bean id="gugudan" class="com.koreait.quiz1.Gugudan">
	<property name="startDan" value="3"></property>
	<property name="endDan" value="4"></property>
	<property name="startNum" value="5"></property>
	<property name="endNum" value="7"></property>
	<property name="calculator" ref="calculator"></property>
</bean>

</beans>
